{
  "cli": {
    "version": ">= 16.12.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
        "production": {
      "developmentClient": true,
      "autoIncrement": true,
      "env": {
        "EXPO_USE_COMMUNITY_AUTOLINKING": "1",
        "EXPO_NO_CAPABILITY_SYNC": "1"
      },
      "android": {
        "image": "sdk-53",
        "gradleCommand": ":app:bundleRelease -x lint",
        "cache": {
          "disabled": true
        },
        "buildType": "app-bundle"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
